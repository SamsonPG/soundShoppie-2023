<% if (locals.session && locals.session.admin) { %>
<%-include('./layout/partials/adminHeader') %> <% } else { %>
<%-include('./layout/partials/userHeader') %> <% } %>
<div
  class="d-flex justify-content-center align-items-center flex-column mt-5 text-center"
>
  <div>
    <form class="add-product" action="/">
      <div class="card-body">
        <h3 class="card-title">404</h3>
        <p class="h5"><span class="text-danger">Oops!</span> Page not found.</p>
        <p class="h5">The page you're looking for doesn't exist.</p>
        <p><%= error %></p>

        <button
          type="button"
          class="btn btn-primary card-details-btn border-0"
          onClick="previous()"
          id="backward"
          style="background-color: #c9bc43; border: none; font-size: 20px"
        >
          Go Back
        </button>
      </div>
    </form>
  </div>
</div>
<% if (locals.session && locals.session.admin) { %>
<%-include('./layout/partials/adminFooter') %> <% } else { %>
<%-include('./layout/partials/userFooter') %> <% } %>
